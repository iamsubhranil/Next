class MyClass {
    pub:
        a, b, c, d
        new(x) {
            a = x++
            b = ++x
            c = x--
            d = --x
        }
}
start = clock()
j = nil
for(i in range(20000)) {
    j = MyClass(i + 1)
}
print("Final object : ", j, " {", j.a, ", ", j.b, ", ", j.c, ", ", j.d, "}\n")
print("elapsed: ", (clock() - start) / clocks_per_sec)
